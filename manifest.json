{
  "manifest_version": 2,
  "name": "Drupal Cache",
  "description": "Informations about Drupal's cache headers",
  "version": "0.0.1",
  "icons": {
    "32": "icons/drupal.png"
  },
  "permissions": [
    "webRequest", "<all_urls>"
  ],
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/drupal.png",
    "default_popup": "browserAction/index.html",
    "default_title": "Drupal Cache"
  }
}